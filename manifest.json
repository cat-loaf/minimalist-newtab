{
    "manifest_version": 3,
    "name": "NewTab by catloaf",
    "version": "1.0",
    "description": "Clean new tab",
    "author": "catloaf",
    "chrome_url_overrides" : {
      "newtab": "index.html"
    },
    "permissions": [
      "storage"
    ],

    "icons": {
      "16":  "icons/icon16.png",
      "48":  "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_icon": {
        "16":  "icons/icon16.png",
        "48":  "icons/icon48.png",
        "128": "icons/icon128.png"
      },
      "default_title": "NewTab"
    }
  }